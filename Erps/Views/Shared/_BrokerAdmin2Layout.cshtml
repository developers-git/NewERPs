                                        var selectQueryString2 = "Select * from Permission  where assignedRole='BrokerAdmin2' and Module='" + forinner + "'  order by Name asc";
                                        <ul class="nav nav-second-level">
                                            @foreach (var row2 in db.Query(selectQueryString2))
